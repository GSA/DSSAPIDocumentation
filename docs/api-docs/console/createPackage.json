		"/Packages/CreateMultiPart": {
			"post": {
				"tags": ["Packages"],
				"summary": "Multipart Upload Package creation",
				"description": "This API enables applications to initiate the digital signature signing ceremony. The API is provisioned as a RESTful service and uses JSON as the data interchange format. Document and attachment files can be attached separately and sent as another key value pair.",
				"operationId": "dssUniversalConnectorMultiPart",
				"consumes": ["multipart/form-data"],
				"produces": ["application/json"],
				"parameters": [{
					"name": "api_key",
					"in": "query",
					"description": "API key",
					"required": true,
					"type": "string",
					"default": "DEMO_KEY"

				}, {
					"in": "formData",
					"name": "document[]",
					"description": "Upload file.",
					"required": true,
					"type": "file"
				}, 
				 {
					"name": "documentDetail[]",
					"in": "formData",
					"description": "File Name",
					"required": true,
					"type": "string",
					"default": "Sample.pdf"

				},{
					"name": "body",
					"in": "formData",
					"description": "JSON Data",
					"required": true,
					"type": "string",
					"format": "string",
					"default": "{\"orgName\": \"IACP\",\"documents\": [{\"document\": {\"signers\": [{\"signerEmail\": \"dss_team@gsa.gov\",\"signerFirstName\": \"firstName\",\"signerLastName\": \"lastName\",\"searchText\": \"LESSOR\",\"signType\": \"ClickToInitial\",\"signaturePosition\": \"TOPRIGHT\"}],\"documentName\": \"TEST-AAAP-Short.PDF\"}}],\"senderEmail\": \"dssteam@gsa.gov\",\"enableSigningOrder\": true,\"packageOption\": \"create\",\"packageName\": \"DSSAPIDocTestSample\"}"
				}],
				"responses": {
					"default": {
						"description": "successful operation"
					}
				}
			}
		},
		"/Packages/CreateWithDocumentLayoutMultipart": {
			"post": {
				"tags": ["Packages"],
				"summary": "DSS create Package from layout multipart file transfer",
				"description": "This API enables applications to initiate the digital signature signing ceremony. The API is provisioned as a RESTful service and uses JSON as the data interchange format. Document and attachment  files can be attached separately and sent as another key value pair.",
				"operationId": "dssCreatePackageFromDocLayoutMultipart",
				"consumes": ["multipart/form-data"],
				"produces": ["application/json"],
				"parameters": [{
					"name": "api_key",
					"in": "query",
					"description": "API key",
					"required": true,
					"type": "string",
					"default": "DEMO_KEY"

				}, {
					"in": "formData",
					"name": "document",
					"description": "The file to upload.",
					"required": true,
					"type": "file"	
					
				}, {
					"name": "body",
					"in": "body",
					"description": "JSON Data",
					"required": true,
					"type": "string",
					"format": "string",
					"default": "{\"orgName\": \"IACP\",\"documents\": [{\"document\": {\"signers\": [{\"signerEmail\": \"dss_test@gsa.gov\",\"signerFirstName\": \"firstName\",\"signerLastName\": \"lastName\",\"placeHolderName\": \"ContractingOfficer\"}],\"documentName\": \"TEST-AAAP-Short.PDF\",\"layoutName\": \"Fleet STR SF1449 Layout\"}}],\"senderEmail\": \"dssteam@gsa.gov\",\"enableSigningOrder\": true,\"packageOption\": \"create\",\"packageName\": \"DSSAPIDocSample2\"}"
				}],
				"responses": {
					"default": {
						"description": "successful operation"
					}
				}
			}
		},
