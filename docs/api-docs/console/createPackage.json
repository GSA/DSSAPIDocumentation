				"produces": ["application/json"],
				"parameters": [{
					"name": "api_key",
					"in": "query",
					"description": "API key for api.gsa.gov",
					"required": false,
					"type": "string",
					"default": "DEMO_KEY"
				},{
					"in": "body",
					"name": "body",
					"description": "JSon Data for DSS create package",
					"required": true
				}],
				"responses": {
					"default": {
						"description": "successful operation"
					}
				}
