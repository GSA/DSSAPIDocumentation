				"operationId": "dssCreatePackageFromDocLayout",
				"consumes": ["application/json"],
				"produces": ["application/json"],
				"parameters": [{
					"name": "api_key",
					"in": "query",
					"description": "API key",
					"required": true,
					"type": "string",
					"default": "DEMO_KEY"

				}, {
					"in": "body",
					"name": "body",
